@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Master>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
}

<html>
<head>
    <!-- Metadata -->
    <title>@Umbraco.Field("Seitentitel", recursive: true) | @Umbraco.Field("seitentitelAnhang", recursive: true)</title>
    <meta name="description" content="@Umbraco.Field("description", recursive: true)">
    @{
        string keywordsFormatted = "";
        if (Model.Content.HasValue("keywords"))
        {
            if (Model.Content.Keywords != null)
            {
                foreach (var tag in Model.Content.Keywords)
                {
                    keywordsFormatted += (keywordsFormatted == "") ? tag : "," + tag;
                }
            }
        }
    }
    <meta name="keywords" content="@keywordsFormatted">
<!-- Bootstrap -->
<link href="/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
<link href="/css/bootstrap.css" rel='stylesheet' type='text/css' />
<meta name="viewport" content="width=device-width, initial-scale=1">
<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
<!-- Custom -->
<link href="/css/style.css" rel="stylesheet" type="text/css" media="all" />
<!-- rte -->
<link href="/css/rte.css" rel="stylesheet" type="text/css" media="all" />
<!----font-Awesome----->
   	<link rel="stylesheet" href="/fonts/css/font-awesome.min.css">
<!----font-Awesome----->
<!-- start plugins -->
<script type="text/javascript" src="/js/jquery.min.js"></script>
<script type="text/javascript" src="/js/bootstrap.js"></script>
<script type="text/javascript" src="/js/bootstrap.min.js"></script>
<!--start slider -->
    <script src="/js/jquery-ui.min.js"></script>
    <script src="/js/css3-mediaqueries.js"></script>
<!--end slider -->
<!-- must have -->
<script src="/js/jquery-ui-1.8.16.custom.min.js" type="text/javascript"></script>
<script src="/js/jquery.ui.touch-punch.min.js" type="text/javascript"></script>
<!--[if IE]><script src="js/excanvas.compiled.js" type="text/javascript"></script><![endif]-->

</head>
<body>

@Html.Partial("Navigation")
@RenderBody()
@Html.Partial("Footer")


</body>
</html>